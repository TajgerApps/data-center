{% extends 'base.html.twig' %}

{% block title %}Order{% endblock %}

{% block body %}
    <h1>Order</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ order.id }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ order.number }}</td>
            </tr>
            <tr>
                <th>Contract_number</th>
                <td>{{ order.contractNumber }}</td>
            </tr>
            <tr>
                <th>Offer_number</th>
                <td>{{ order.offerNumber }}</td>
            </tr>
            <tr>
                <th>Investor</th>
                <td>{{ order.investor }}</td>
            </tr>
            <tr>
                <th>Object_type</th>
                <td>{{ order.objectType }}</td>
            </tr>
            <tr>
                <th>Building_category</th>
                <td>{{ order.buildingCategory }}</td>
            </tr>
            <tr>
                <th>Safety</th>
                <td>{{ order.safety }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('order_index') }}">back to list</a>

    <a href="{{ path('order_edit', {'id': order.id}) }}">edit</a>

    {{ include('order/_delete_form.html.twig') }}
{% endblock %}
